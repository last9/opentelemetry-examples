{
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "open-telemetry/opentelemetry": "^1.0",
        "php-http/httplug": "^2.4",
        "open-telemetry/exporter-otlp": "^1.1",
        "php-http/guzzle7-adapter": "^1.1",
        "open-telemetry/opentelemetry-auto-curl": "^0.0.2",
        "open-telemetry/opentelemetry-auto-guzzle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "OpenTelemetry\\Contrib\\Instrumentation\\MySqli\\": "src/OpenTelemetry/Contrib/Instrumentation/MySqli/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    }
}
