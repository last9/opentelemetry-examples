# Last9 OpenTelemetry Configuration
# Copy this file to .env and fill in your credentials

# OTLP Endpoint for Last9 (use the region-specific endpoint)
OTEL_EXPORTER_OTLP_ENDPOINT=<last9 otlp endpoint>

# Authorization header for Last9
OTEL_EXPORTER_OTLP_HEADERS=Authorization=Basic YOUR_BASE64_AUTH_KEY

# Service name for identification
OTEL_SERVICE_NAME=cpp-sample-app

# Deployment environment
DEPLOYMENT_ENVIRONMENT=local
