{
  "name": "node14-express-otel-example",
  "version": "1.0.0",
  "description": "Node.js 14 Express example with OpenTelemetry - traces, metrics, and resource detection",
  "main": "app.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node -r ./instrumentation.js app.js",
    "start:db-examples": "node -r ./instrumentation.js db-examples.js",
    "test": "curl http://localhost:3000"
  },
  "dependencies": {
    "express": "4.18.2",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/sdk-node": "0.52.1",
    "@opentelemetry/sdk-trace-node": "1.26.0",
    "@opentelemetry/sdk-metrics": "1.26.0",
    "@opentelemetry/resources": "1.26.0",
    "@opentelemetry/semantic-conventions": "1.27.0",
    "@opentelemetry/auto-instrumentations-node": "0.49.1",
    "@opentelemetry/exporter-trace-otlp-http": "0.52.1",
    "@opentelemetry/exporter-metrics-otlp-http": "0.52.1",
    "@opentelemetry/instrumentation-runtime-node": "0.6.0",
    "@opentelemetry/resource-detector-container": "0.4.2",
    "@opentelemetry/resource-detector-aws": "1.6.1"
  },
  "optionalDependencies": {
    "pg": "^8.11.0",
    "mysql2": "^3.3.0",
    "mongodb": "^5.5.0",
    "redis": "^4.6.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {},
  "keywords": [
    "opentelemetry",
    "node14",
    "express",
    "observability",
    "tracing",
    "metrics",
    "last9"
  ]
}
