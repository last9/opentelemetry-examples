# PostgreSQL Connection
# RDS endpoint (e.g., my-db.xxxx.region.rds.amazonaws.com)
PG_ENDPOINT=<your-rds-endpoint>
PG_PORT=5432
PG_USERNAME=otel_monitor
PG_PASSWORD=<your-secure-password>
PG_DATABASE=<your-database-name>

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=<your-access-key>
AWS_SECRET_ACCESS_KEY=<your-secret-key>
# RDS Instance Identifier (not the endpoint, just the identifier)
RDS_INSTANCE_ID=<your-rds-instance-id>

# Last9 Configuration
# Get these from Last9 Integrations page
LAST9_OTLP_ENDPOINT=YOUR_LAST9_ENDPOINT
LAST9_AUTH_HEADER=Basic <your-base64-encoded-credentials>

# Environment (prod, staging, dev)
ENVIRONMENT=dev
